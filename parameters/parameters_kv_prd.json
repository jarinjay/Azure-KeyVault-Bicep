{
	"$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"environment": {
			"value": "prd"
		},
        	"virtualNetworkRulesid": {
			"value": "<VNET RESOURCE ID>/subnets/<SUBNET NAME>"
		},
		"accessPoliciesObject": {
			"value": {
				"accessPolicies": [
					{"objectId": "{objectId1}"},
					{"objectId": "{objectId2}"}
				]
			}
		},
		"ipRulesList":{
			"value": [
			    "IP ADDRESS",
			    "IP ADDRESS RANGE"
			]
		}
    } 
}
